<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="138.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="600.0" spacing="10.0">
         <children>
            <CheckBox fx:id="garantiaCheckBox" mnemonicParsing="false" prefHeight="18.0" prefWidth="97.0" text="Garantia" />
            <RadioButton fx:id="imovelRadio" disable="true" mnemonicParsing="false" prefHeight="18.0" prefWidth="89.0" text="Imovel" />
            <RadioButton fx:id="automovelRadio" disable="true" mnemonicParsing="false" prefHeight="18.0" prefWidth="114.0" selected="true" text="Automovel" />
            <RadioButton fx:id="outroBemRadio" disable="true" mnemonicParsing="false" text="Outro Bem" />
         </children>
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="41.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label fx:id="bemLabel" text="Escolha o automovel:" />
            <ComboBox fx:id="bemCombo" prefWidth="150.0" promptText="-" />
            <Button fx:id="novoBemBtn" mnemonicParsing="false" onAction="#novoBemBtnClick" prefHeight="26.0" prefWidth="145.0" text="Novo Automovel" />
         </children>
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER_LEFT" prefHeight="65.0" prefWidth="600.0" spacing="10.0">
         <children>
            <Label text="Descricao:" />
            <TextField fx:id="descricaoText" prefHeight="26.0" prefWidth="448.0" />
         </children>
         <padding>
            <Insets bottom="5.0" left="20.0" right="20.0" top="5.0" />
         </padding></HBox>
   </children>
</VBox>
